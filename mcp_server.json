{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@0.0.38",
        "--browser=",
        "--headless=",
        "--viewport-size="
      ]
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@0.2.3"
      ],
      "env": {
        "TAVILY_API_KEY": "https://mcp.tavily.com/mcp/?tavilyApiKey=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"
      }
    }
  }
}
